class Animal
{
    speak()
    {
        print("From base class" + "\n");
    }
}

class Dog < Animal
{
    init(name)
    {
        this.name = name;
        this.x = 0;
    }
    speak()
    {
        super.speak();
        print(this.name + " says " + " woof" + "\n");
    }
}

let scooby = Dog("Scooby");
scooby.x = 10;
scooby.speak();