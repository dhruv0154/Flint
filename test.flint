class Cake 
{
  init()
  { 
    this.flavor = "abc";
  }
  taste() 
  {
    let adjective = "delicious";
    print ("The " + this.flavor + " cake is " + adjective + "!");
  }
}
let cake = Cake();
cake.flavor;
cake.taste(); // Prints "The German chocolate cake is delicious!".
let cake2 = Cake();
cake2.flavor = "Strawberry";
cake2.taste();